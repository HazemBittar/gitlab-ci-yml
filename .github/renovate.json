{
  "extends": [
    "github>SocialGouv/renovate-config",
    ":automergeAll"
  ],
  "gitlabci": {
    "fileMatch": ["(^|/)[^/]*\\.ya?ml$"]
  },
  "packageRules": [
    {
      "depTypeList": ["final", "stage", "image-name"],
      "updateTypes": ["major", "minor"],
      "semanticCommitType": "feat"
    }
  ]
}
